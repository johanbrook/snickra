{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "object-shorthand": 1,
    "no-new": 0
  },
  "plugins": [
    "html"
  ]
}
